  "scripts": {
    "start": "craco start",
    "start:subdomains": "DANGEROUSLY_DISABLE_HOST_CHECK=true craco start",
    "build": "INLINE_RUNTIME_CHUNK=false craco build",
    "cypress:open": "cypress open",
    "cypress:open:component": "cypress open-ct",
    "cypress:run": "cypress run",
    "cypress:run:component": "cypress run-ct",
    "test": "react-scripts test",
    "test:coverage": "CI=true npm test -- --env=jsdom --coverage",
    "test:ci": "CI=true npm test -- --env=jsdom"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(test).[jt]s?(x)"
    ],
    "collectCoverageFrom": [
      "**/*.test.{js,jsx}",
      "!**/node_modules/**",
      "!**/*.styled.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(cypress)/)",
      "\\.pnp\\.[^\\\/]+$"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier",
      "plugin:chai-friendly/recommended"
    ],
    "env": {
      "cypress/globals": true
    },
    "plugins": [
      "cypress",
      "chai-friendly"
    ],
    "overrides": [
      {
        "files": [
          "*.spec.js"
        ],
        "rules": {
          "jest/valid-expect": 0,
          "jest/valid-expect-in-promise": 0
        }
      }
    ]
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "IE 10"
  ],
  "devDependencies": {
    "@cypress/react": "^5.9.4",
    "@cypress/webpack-dev-server": "1.7.0",
    "cypress": "^8.5.0",
    "eslint-plugin-chai-friendly": "^0.7.1"
  }
}
