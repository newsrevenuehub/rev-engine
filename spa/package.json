  "scripts": {
    "start": "craco start",
    "start:subdomains": "DANGEROUSLY_DISABLE_HOST_CHECK=true craco start",
    "build": "INLINE_RUNTIME_CHUNK=false craco build",
    "build:low-memory": "INLINE_RUNTIME_CHUNK=false NODE_ENV=production craco --max_old_space_size=2560 --optimize_for_size build",
    "chromatic": "chromatic --exit-zero-on-changes",
    "cypress:open": "cypress open --e2e",
    "cypress:run": "cypress run --e2e",
    "test": "react-scripts test",
    "test:coverage": "CI=true npm test -- --env=jsdom --coverage",
    "test:ci": "CI=true npm test -- --env=jsdom",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(test).[jt]s?(x)"
    ],
    "collectCoverageFrom": [
      "**/*.[jt]s?(x)",
      "!**/node_modules/**",
      "!**/*.stories.[jt]sx",
      "!**/*.styled.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(cypress|url-join)/)",
      "\\.pnp\\.[^\\/]+$"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier",
      "plugin:chai-friendly/recommended"
    ],
    "env": {
      "cypress/globals": true
    },
    "plugins": [
      "cypress",
      "chai-friendly"
    ],
    "overrides": [
      {
        "files": [
          "*.spec.js"
        ],
        "rules": {
          "jest/valid-expect": 0,
          "jest/valid-expect-in-promise": 0
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "@cypress/react": "^6.2.0",
    "@cypress/webpack-preprocessor": "^5.12.2",
    "@react-theming/storybook-addon": "^1.1.7",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
